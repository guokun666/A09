<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.boot.demo.dao.HolidayPreditcDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.boot.demo.entity.HolidayPreditcEntity" id="HolidayPreditcMap">
<result property="holiday" column="date_attribute"/>
        <result property="station" column="station_name"/>

        <result property="flow" column="flow"/>

    </resultMap>


    <select id="getCityHoliday" resultMap="HolidayPreditcMap">
        SELECT sum(flow) as flow,date_attribute FROM y_2 group by date_attribute
    </select>
    <select id="getStationHoliday" resultMap="HolidayPreditcMap">
        SELECT * FROM y_2
    </select>


</mapper>